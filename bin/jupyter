#!/usr/bin/env bash


navigate() {
    sleep 5
    open "http://localhost:8888"
}

jupyter_subcmd() {

    local JUPYTER_BIN="docker run --rm -p 8888:8888 -v $(pwd):/usr/share/ butuzov/jupyter:latest "
    local JUPYTER_ARGS="jupyter  notebook --config=/home/jupyter/.jupyter/nbconfig/ --ip=0.0.0.0 --no-browser --port=8888 --log-level DEBUG --allow-root"

     navigate &
        $JUPYTER_BIN
}


jupyter_subcmd $@
